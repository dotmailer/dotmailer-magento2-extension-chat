{
    "name": "dotdigital/dotdigital-magento2-extension-chat",
    "description": "Dotdigital for Magento 2: Chat module",
    "type": "magento2-module",
    "version": "1.7.2",
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    ],
    "replace": {
        "dotmailer/dotmailer-magento2-extension-chat": "*"
    },
    "require": {
        "php": "^7.4||^8.0",
        "dotdigital/dotdigital-magento2-extension": "^4.18.0",
        "magento/framework": ">=102 <104",
        "magento/module-config": ">=101 <102",
        "magento/module-backend": ">=101 <103",
        "magento/module-customer": ">=102 <104",
        "magento/module-store": ">=101 <102",
        "magento/module-checkout": "^100.3|^100.4",
        "magento/module-quote": ">=101 <103",
        "magento/module-sales": ">=102 <104",
        "ext-json": "*"
    },
    "autoload": {
        "files": [ "registration.php" ],
        "psr-4": {
            "Dotdigitalgroup\\Chat\\": ""
        }
    }
}
